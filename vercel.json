{
    "buildCommand": "npm run vercel-build",
    "outputDirectory": "dist",
    "framework": null,
    "installCommand": "npm install --legacy-peer-deps",
    "functions": {
        "app/api/**/*.js": {
            "runtime": "nodejs18.x"
        }
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}